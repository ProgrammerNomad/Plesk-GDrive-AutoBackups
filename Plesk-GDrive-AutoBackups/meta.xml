<extension>
  <id>plesk-gdrive-autobackups</id>
  <name>Plesk GDrive AutoBackups</name>
  <version>1.0.0</version>
  <description>Automatically back up your Plesk server data to Google Drive.</description>
  <vendor>Nomad Programmer</vendor>
  <packager>Nomad Programmer</packager> 
  <homepage>https://github.com/ProgrammerNomad/Plesk-GDrive-AutoBackups</homepage>

  <interface>
    <main>htdocs/index.php</main>
    <remote-storage-settings>htdocs/index.php</remote-storage-settings>
  </interface>

  <dependencies>
    <plesk-version>18.0.0</plesk-version>
  </dependencies>
</extension>